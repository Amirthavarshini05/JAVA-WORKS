//SIMPLE CALCULATOR

//PROGRAM:

import java.util.Scanner;

class calculate
{
	public static void main(String[] args)
	{
		int a,b;
		Scanner s =new Scanner(System.in);
		a=s.nextInt();
		b=s.nextInt();
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.printf("%.2f\n",((float)a/(float)(b)));
		System.out.println(a%b);
	}
}


/*SAMPLE INPUT:

7
2


SAMPLE OUTPUT:

9
5
14
3.50
1
*/	
