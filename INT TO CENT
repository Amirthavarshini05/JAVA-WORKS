import java.util.Scanner;

public class centA
{
	public static void main(String[] args)
	{
		Scanner s = new Scanner(System.in);
		int a= s.nextInt();
		int b = s.nextInt();
		double c= a/100.0;
		double d = b/100.0;
		System.out.printf("%.2f",c);
		System.out.println();
		System.out.printf("%.2f",d);
	}
